{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "A node structure copied to the clipboard",
  "type": "object",
  "properties": {
    "copiedNodes": {
      "type": "object",
      "patternProperties": {
        "^[0-9a-zA-Z_]+$": {
          "type": "object",
          "$ref": "base_node.json",
        }
      }
    },
    "width": {
      "type": "number"
    },
    "height": {
      "type": "number"
    },
    "version": {
      "type": "number"
    },
    "variables": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "conditions": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "dataType": {
      "type": "string"
    }
  },
  "required": [
    "copiedNodes",
    "width",
    "height",
    "version",
    "variables",
    "conditions",
    "dataType"
  ]
}
